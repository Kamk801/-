//树的存储结构
//双亲表示法
#include<stdlib.h>
#define Maxsize 100
typedef struct{
    int data;
    int parent;
}TNode;
typedef struct {
    TNode node[Maxsize];
    int n;                 //实际存储
}Tree;
//孩子表示法
typedef struct {
    int data;
    Node* next;
}Node;
typedef struct {
    int data;
    Node* firstchild;
}head;
typedef struct {
    head  firstnode[Maxsize];
    int n，r;                 //实际存储
}tree;
//孩子兄弟表示法
typedef struct {
    int data;
    CSNode* child;
    CSNode* bro;
}CSNode,*CSTree;
