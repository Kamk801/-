//中序前驱
//求前驱结点
#include<stdio.h>
#include<stdlib.h>
typedef struct tree{
	int data;
	TreeNode *lchild;   //三叉链表
	TreeNode* rchild;
	int ltag, rtag ; //1为线索   0为孩子
}TreeNode,*Tree;
TreeNode* pre = NULL;
TreeNode* final = NULL;
TreeNode* p;
void createinthread(Tree T) {

}
void inthread(Tree T) {
	if (T != NULL) {
		inthread(T->lchild);
		visit(T, p);
		inthread(T->rchild);
	}
}
void visit(TreeNode* leaf, TreeNode* p) {
	if (leaf == p) {
		final = pre;
	}
	else {
		pre = p;
	}
}
