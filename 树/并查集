#include<stdlib.h>
#include<stdio.h>
//并查集
#define Maxsize 100
int resemble[Maxsize];
//初始化
void init() {
	for (int i = 0; i < Maxsize; i++) {
		resemble[i] = -1;
	}
}
//查    x结点的根结点是谁
int find(int x) {
	int root = x;
	while (resemble[root] >= 0) {
		root = resemble[root];
	}
	while (resemble[x] != root) {
		int t = resemble[x];
		resemble[x] = root;
		x = t;
	}
	return root;
}
//并
void Union(int root1, int root2) {
	if (root1 == root2) return;
	if (root2 > root1) {
		resemble[root1] += resemble[root2];
		resemble[root2] = root1;
	}
	else {
		resemble[root2] += resemble[root1];
		resemble[root1] = root2;
	}

}
