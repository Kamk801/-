//顺序队列  
#include<stdio.h>
#include<stdlib.h>
#define Maxsize 10
typedef struct {
	int data[Maxsize];
	int front;
	int rear;
	int size;
}SqQueue;
// 初始化
void InitQueue(SqQueue& sq) {
	sq.rear=sq.front = 0;
	sq.size = 0;
}
//判空  
bool QueueEmpty(SqQueue sq) {
	if (sq.size==0) {
		return true;
	}
	else {
		return false;
	}
}
//判满
bool QueueFull(SqQueue sq) {
	if (sq.size==Maxsize) {
		return true;
	}
	else {
		return false;
	}
}
//入队
bool EnQueue(SqQueue& sq, int elem) {
	if (sq.size==Maxsize) {     //队列已满
		return false;
	}
	sq.data[sq.rear] = elem;
	sq.rear = (sq.rear + 1) % Maxsize;
	sq.size++;
	return true;
}
//出队
bool DeQueue(SqQueue& sq, int& num) {
	if (sq.size==0) {
		return false;
	}
	int num = sq.data[sq.front];
	sq.front = (sq.front + 1) % Maxsize;
	sq.size--;
	return true;
}
//获得队头元素
bool GetHead(SqQueue& sq, int& num) {
	if (sq.size==0) {
		return false;
	}
	int num = sq.data[sq.front];
	return true;
}
